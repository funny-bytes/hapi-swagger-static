{
  "extends": [
    "airbnb"
  ],
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true,
    "mocha": true
    },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "react/prop-types": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "react/no-array-index-key": 0
  }
}
