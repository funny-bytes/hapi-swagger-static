{
  "name": "hapi-swagger-static",
  "version": "1.0.3",
  "description": "A small companion plugin for ´hapi-swagger´ providing a static html documentation page as hapi route",
  "main": "src/index.js",
  "author": "Frank Thelen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/frankthelen/hapi-swagger-static"
  },
  "keywords": [
    "hapi-swagger",
    "swagger",
    "hapi",
    "static",
    "html",
    "page"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path ./.eslintignore",
    "test": "NODE_ENV=test mocha --recursive --timeout 10000 test",
    "cover": "NODE_ENV=test istanbul cover _mocha -- --recursive --timeout 10000 test",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "peerDependencies": {
    "hapi": ">=17.x.x",
    "hapi-swagger": ">=9.x.x"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-should-promised": "^2.0.0",
    "hapi": "^17.2.0",
    "hapi-swagger": "^9.0.2",
    "inert": "^5.1.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^4.1.6",
    "sinon-chai": "^2.14.0",
    "vision": "^5.3.1"
  },
  "dependencies": {
    "boom": "^7.1.1",
    "bootprint": "^1.0.2",
    "bootprint-openapi": "^1.1.0",
    "html-inline": "^1.2.0",
    "tmp-promise": "^1.0.4"
  }
}
