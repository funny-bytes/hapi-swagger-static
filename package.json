{
  "name": "hapi-swagger-static",
  "version": "2.0.12",
  "description": "A small companion plugin for `hapi-swagger` or `hapi-swaggered` providing a static html documentation page as hapi route",
  "main": "src/index.js",
  "author": "Frank Thelen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/frankthelen/hapi-swagger-static"
  },
  "keywords": [
    "hapi-swagger",
    "swagger",
    "hapi",
    "static",
    "html",
    "page"
  ],
  "scripts": {
    "lint": "eslint . --ignore-path ./.eslintignore",
    "test": "NODE_ENV=test mocha --recursive test",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive test",
    "coveralls": "npm run cover && nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "peerDependencies": {
    "@hapi/hapi": ">=17.x.x",
    "@hapi/vision": ">=5.x.x"
  },
  "devDependencies": {
    "@hapi/hapi": "^19.0.0",
    "@hapi/inert": "^5.2.1",
    "@hapi/vision": "^5.5.2",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-should-promised": "^2.0.0",
    "hapi-swagger": "^10.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "bluebird": "^3.5.5",
    "openapi2html": "^1.1.11",
    "swagger-parser": "^8.0.0"
  }
}
